    <script>
        const timerElement = document.getElementById('timer');
        const openButton = document.getElementById('openButton');
        const progressBar = document.getElementById('progress-bar');
        let countdown = 10;
        
        function updateCountdown() {
            countdown--;
            timerElement.textContent = countdown;
            const progress = (10 - countdown) * 10;
            progressBar.style.width = progress + '%';
            
            if (countdown <= 0) {
                clearInterval(timerInterval);
                timerElement.textContent = 'Go!';
                openButton.style.display = 'flex';
                document.querySelector('.timer-wrapper').innerHTML = 'Ready!';
            }
        }
        
        const timerInterval = setInterval(updateCountdown, 1000);
        
        openButton.addEventListener('click', function() {
            window.open("https://www.effectivecpmrate.com/v7udtj04e?key=ebdce65ede7af7e8d3b2fe281f04283a", '_blank');
        });
    </script>
    
